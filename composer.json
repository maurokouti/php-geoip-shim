{
    "name": "maurokuoti/php-geoip-shim",
    "description": "PHP GeoIP shim to support legacy geoip_* functions with the GeoIP2 .mmdb database files.",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "geoip2/geoip2": "^2.11"
    },
    "authors": [
        {
            "name": "Mauro Kouti",
            "email": "maurokuoti@protonmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": { "GeoIPShim\\": "src/" },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "GeoIPShim\\Tests\\": "tests/" }
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^0.12.96"
    }
}
